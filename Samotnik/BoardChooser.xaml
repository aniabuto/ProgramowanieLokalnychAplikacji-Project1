<Window x:Class="Project1.BoardChooser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:uc="clr-namespace:Project1"
        xmlns:local="clr-namespace:Project1"
        mc:Ignorable="d"
        Title="BoardChooser" Height="370" Width="640" ResizeMode="NoResize">
    <Grid>
        <uc:OptionButton x:Name="BoardChoose" BoardChosen="BoardChoose_OnBoardChosen"></uc:OptionButton>
    </Grid>
</Window>


